# Example .env file for Git Repository to Pinecone Ingestion Pipeline
# Copy this file to .env and fill in your actual API keys

# Pinecone API credentials
# Get these from your Pinecone dashboard: https://app.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here  # e.g., us-west1-gcp

# Firecrawl API credentials
# Get this from your Firecrawl account
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Embedding API credentials (if using an API-based embedding model)
# This depends on which embedding model you're using in config.yaml
EMBEDDING_API_KEY=your_embedding_api_key_here

# Optional: Custom configuration
# Uncomment and modify these if you want to override defaults

# PINECONE_INDEX_NAME=custom_index_name  # Default is the repository name
# EMBEDDING_MODEL=custom_embedding_model  # Default is from config.yaml
# MAX_CHUNK_SIZE=1000  # Default is 500 characters for Firecrawl content
